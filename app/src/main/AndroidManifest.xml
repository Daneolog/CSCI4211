<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="user.com.csci4211">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity"/>
        <activity android:name=".HeartRateActivity"/>
        <activity android:name=".StepCountActivity"/>
        <activity android:name=".DashboardActivity"/>
        <activity android:name=".BreathingActivity"/>
        <activity android:name=".StepCountHistoryActivity"/>
        <activity android:name=".JournalActivity">
        </activity>
    </application>

</manifest>